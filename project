#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<conio.h>
#define AdminName "mesbah"
#define Password "12345"

void welcome();

void main()

{
    char user_name[10],pass[5];
    printf("\t\t\t\t\t ---------------Welcome---------------\n");
    printf("\n\t\t\t\t\t**********please log in first**********\n");
again:
    printf("\n\n\t\t\t\t\tUser Name:\n");
    gets(user_name);

    printf("\n\t\t\t\t\tPassword:\n");
    for(int i=0;i<5;i++){
        char x=getch();
        pass[i]=x;
        printf("*");
    }

    if(strcmp(user_name,AdminName)==0 && strcmp(pass,Password)==0)
        welcome();
    else
        goto again;
}

welcome()
{
    system("cls");
    printf("\n\t\t\t\t\t\t\t\t\t\tYou are succefully login\n");
}
